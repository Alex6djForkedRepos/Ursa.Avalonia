                                                <TextBox MinHeight="20" />
                                            </DockPanel>
                                            <DockPanel Width="{Binding ItemSelfWidth}"
                                                       Height="{Binding ItemSelfHeight}">
                                                <TextBlock VerticalAlignment="Center" Text="Content 4" />
                                                <TextBox MinHeight="20" />
                                            </DockPanel>
                                            <DockPanel Width="{Binding ItemSelfWidth}"
                                                       Height="{Binding ItemSelfHeight}">
                                                <TextBlock VerticalAlignment="Center" Text="Content 5" />
                                                <TextBox MinHeight="20" />
                                            </DockPanel>
                                            <DockPanel Width="{Binding ItemSelfWidth}"
